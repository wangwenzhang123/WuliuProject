<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/_ffe06e38">

        <ImageView
            android:id="@+id/register_back"
            android:layout_width="@dimen/dp_9"
            android:layout_height="@dimen/dp_18"
            android:layout_marginBottom="@dimen/dp_15"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_15"
            android:src="@drawable/back" />

        <TextView
            android:id="@+id/back_tv"
            android:textColor="@color/FFFFFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_7"
            android:layout_marginTop="@dimen/dp_11"
            android:layout_toEndOf="@+id/register_back"
            android:layout_toRightOf="@+id/register_back"
            android:text="@string/back"
            android:textSize="@dimen/sp_18" />

        <TextView
            android:textColor="@color/FFFFFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/dp_10"
            android:text="@string/add_car"
            android:textSize="@dimen/sp_18" />
    </RelativeLayout>

    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_marginTop="@dimen/dp_13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_marginLeft="@dimen/dp_13"
                    android:layout_gravity="center"
                    android:text="@string/update_car_pic"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <ImageView
                    android:id="@+id/user_logo"
                    android:padding="@dimen/dp_5"
                    android:src="@mipmap/default_logo"
                    android:layout_marginLeft="@dimen/dp_8"
                    android:layout_width="@dimen/dp_69"
                    android:layout_height="@dimen/dp_69" />
            </LinearLayout>
            <View
                android:layout_marginTop="@dimen/dp_4"
                android:background="@color/_E0E0E0"
                android:layout_width="match_parent"
                android:layout_height="15dp">

            </View>
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_44"
                android:layout_marginTop="@dimen/dp_10"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|left"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:text="@string/car_no"
                    android:textColor="@color/_000000"
                    android:textSize="@dimen/sp_14" />

                <EditText
                    android:id="@+id/et_car_no"
                    android:layout_width="@dimen/dp_290"
                    android:layout_height="@dimen/dp_30"
                    android:layout_gravity="right|center"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@null"
                    android:gravity="right|center"
                    android:hint="请输入车牌号码"
                    android:textSize="@dimen/sp_14" />
            </FrameLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_marginRight="@dimen/dp_16"
                android:background="@color/_333333">

            </View>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_44"

                android:orientation="horizontal">
                <TextView
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|left"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:text="@string/driver"
                    android:textColor="@color/_000000"
                    android:textSize="@dimen/sp_14" />
                <TextView
                    android:id="@+id/driver_name"
                    android:layout_width="@dimen/dp_290"
                    android:layout_height="@dimen/dp_30"
                    android:layout_gravity="right|center"
                    android:background="@null"
                    android:gravity="right|center"
                    android:layout_marginRight="@dimen/dp_5"
                    android:textSize="@dimen/sp_14" />
                <ImageView
                    android:layout_marginRight="@dimen/dp_16"
                    android:src="@mipmap/go"
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_marginRight="@dimen/dp_16"
                android:background="@color/_333333">

            </View>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:textColor="@color/_333333"
                    android:id="@+id/issue_vehicle_type_tv"
                    android:text="@string/vehicle_type"
                    android:layout_marginTop="@dimen/dp_12"
                    android:layout_weight="1"
                    android:layout_marginLeft="@dimen/dp_17"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <RadioGroup

                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dp_44">

                    <RadioButton
                        android:id="@+id/rb_tong"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginRight="@dimen/dp_24"
                        android:button="@null"
                        android:checked="true"
                        android:drawableLeft="@drawable/selector_order_home"
                        android:drawablePadding="@dimen/dp_8"
                        android:text="@string/concrete_car" />
                    <RadioButton
                        android:id="@+id/rb_beng"
                        android:layout_marginRight="@dimen/dp_22"
                        android:layout_gravity="center"
                        android:text="@string/pump_truck"
                        android:button="@null"
                        android:drawablePadding="@dimen/dp_8"
                        android:drawableLeft="@drawable/selector_order_home"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RadioGroup>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_marginRight="@dimen/dp_16"
                android:background="@color/_333333">

            </View>
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_44"

                android:orientation="horizontal">

                <EditText
                    android:id="@+id/et_car_brand"
                    android:layout_width="@dimen/dp_290"
                    android:layout_height="@dimen/dp_30"
                    android:layout_gravity="right|center"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@null"
                    android:gravity="right|center"
                    android:hint="请输入车辆品牌"
                    android:textSize="@dimen/sp_14" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|left"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:text="@string/car_brand"
                    android:textColor="@color/_000000"
                    android:textSize="@dimen/sp_14" />
            </FrameLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_marginRight="@dimen/dp_16"
                android:background="@color/_333333">

            </View>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_44"

                android:orientation="horizontal">

                <TextView
                    android:inputType="numberDecimal"
                    android:id="@+id/et_contact_phone"
                    android:layout_width="@dimen/dp_290"
                    android:layout_height="@dimen/dp_30"
                    android:layout_gravity="right|center"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@null"
                    android:gravity="right|center"
                    android:hint="请选择购置时间"
                    android:textSize="@dimen/sp_14" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|left"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:text="@string/purchase_time"
                    android:textColor="@color/_000000"
                    android:textSize="@dimen/sp_14" />
            </FrameLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_marginRight="@dimen/dp_16"
                android:background="@color/_333333">

            </View>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_44"

                android:orientation="horizontal">

                <EditText
                    android:id="@+id/et_car_type"
                    android:layout_width="@dimen/dp_290"
                    android:layout_height="@dimen/dp_30"
                    android:layout_gravity="right|center"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@null"
                    android:gravity="right|center"
                    android:hint="请输入车辆型号"
                    android:textSize="@dimen/sp_14" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|left"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:text="@string/car_type"
                    android:textColor="@color/_000000"
                    android:textSize="@dimen/sp_14" />
            </FrameLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_marginRight="@dimen/dp_16"
                android:background="@color/_333333">

            </View>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_44"

                android:orientation="horizontal">

                <EditText
                    android:id="@+id/et_travlled_distance"
                    android:layout_width="@dimen/dp_290"
                    android:layout_height="@dimen/dp_30"
                    android:layout_gravity="right|center"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@null"
                    android:gravity="right|center"
                    android:hint="请输入车辆行驶里程"
                    android:textSize="@dimen/sp_14" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|left"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:text="@string/travlled_distance"
                    android:textColor="@color/_000000"
                    android:textSize="@dimen/sp_14" />
            </FrameLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_marginRight="@dimen/dp_16"
                android:background="@color/_333333">

            </View>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_44">
                <TextView
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|left"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:text="@string/car_load"
                    android:textColor="@color/_000000"
                    android:textSize="@dimen/sp_14" />
                <EditText
                    android:id="@+id/et_car_load"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dp_30"
                    android:layout_gravity="right|center"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@null"
                    android:gravity="right|center"
                    android:hint="请输入车辆载重"
                    android:textSize="@dimen/sp_14" />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_marginRight="@dimen/dp_16"
                android:background="@color/_333333">

            </View>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_44">
                <TextView
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|left"
                    android:layout_marginLeft="@dimen/dp_16"
                    android:text="@string/car_time"
                    android:textColor="@color/_000000"
                    android:textSize="@dimen/sp_14" />
                <TextView
                    android:id="@+id/car_time"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dp_30"
                    android:layout_gravity="right|center"
                    android:layout_marginRight="@dimen/dp_16"
                    android:background="@null"
                    android:gravity="right|center"
                    android:hint="请选择车辆保险到期时间"
                    android:textSize="@dimen/sp_14" />
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_marginRight="@dimen/dp_16"
                android:background="@color/_333333">

            </View>
            <TextView
                android:textColor="@color/_000000"
                android:layout_marginTop="@dimen/dp_11"
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/upload_documents" />
            <FrameLayout
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_marginTop="@dimen/dp_30"
                android:orientation="horizontal"
                android:layout_width="@dimen/dp_168"
                android:layout_height="@dimen/dp_88">
                <LinearLayout
                    android:id="@+id/ll_business_license"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:layout_width="@dimen/dp_118"
                    android:layout_height="match_parent"
                    android:background="@drawable/shape_dotted_line">
                    <ImageView
                        android:layout_marginTop="@dimen/dp_16"
                        android:layout_gravity="center"
                        android:src="@mipmap/registeradd"
                        android:layout_width="@dimen/dp_26"
                        android:layout_height="@dimen/dp_26" />
                    <TextView
                        android:textSize="@dimen/sp_12"
                        android:text="行驶证上传"
                        android:layout_marginTop="@dimen/dp_8"
                        android:layout_gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <ImageView
                    android:scaleType="fitXY"
                    android:id="@+id/iv_business_license"
                    android:layout_gravity="center"
                    android:layout_width="@dimen/dp_118"
                    android:layout_height="match_parent"
                    android:background="@drawable/shape_dotted_line"
                    />
            </FrameLayout>
            <Button
                android:id="@+id/register_register_bt"
                android:layout_width="@dimen/dp_302"
                android:layout_height="@dimen/dp_42"
                android:layout_below="@+id/view4"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:layout_marginBottom="@dimen/dp_20"
                android:layout_marginTop="@dimen/dp_42"
                android:background="@drawable/shape_login"
                android:text="@string/sure_add"
                android:textColor="@color/FFFFFF"
                android:textSize="@dimen/sp_14" />
        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>

</LinearLayout>