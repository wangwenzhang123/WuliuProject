<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/_ffe06e38">

        <ImageView
            android:padding="@dimen/dp_10"
            android:layout_centerVertical="true"
            android:id="@+id/register_back"
            android:layout_width="@dimen/dp_50"
            android:layout_height="@dimen/dp_45"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:src="@mipmap/white_back" />

        <TextView
            android:id="@+id/back_tv"
            android:textColor="@color/FFFFFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_42"
            android:layout_marginTop="@dimen/dp_11"
            android:text="@string/back"
            android:textSize="@dimen/sp_18" />

        <TextView
            android:textColor="@color/FFFFFF"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/dp_10"
            android:text="@string/add_manager"
            android:textSize="@dimen/sp_18" />
    </RelativeLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_marginTop="@dimen/dp_13"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:textColor="@color/_333333"
            android:layout_weight="1"
            android:layout_marginLeft="@dimen/dp_13"
            android:layout_gravity="center"
            android:text="@string/update_head"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_marginRight="@dimen/dp_10"
            android:id="@+id/user_ico"
            android:padding="@dimen/dp_5"
            android:src="@mipmap/user_defut"
            android:layout_marginLeft="@dimen/dp_8"
            android:layout_width="@dimen/dp_69"
            android:layout_height="@dimen/dp_69" />
    </LinearLayout>
    <View
        android:layout_marginTop="@dimen/dp_4"
        android:layout_marginLeft="@dimen/dp_13"
        android:background="@color/_E0E0E0"
        android:layout_width="match_parent"
        android:layout_height="1dp">

    </View>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44">
        <TextView
            android:textColor="@color/_333333"
            android:layout_marginLeft="@dimen/dp_13"
            android:text="@string/user_name"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <EditText
            android:textSize="@dimen/dp_14"
            android:hint="请输入姓名"
            android:id="@+id/user_name"
            android:layout_marginRight="@dimen/dp_13"
            android:gravity="right"
            android:background="@null"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <View
        android:layout_marginTop="@dimen/dp_4"
        android:layout_marginLeft="@dimen/dp_13"
        android:background="@color/_E0E0E0"
        android:layout_width="match_parent"
        android:layout_height="1dp">

    </View>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44">
        <TextView
            android:textColor="@color/_333333"
            android:layout_marginLeft="@dimen/dp_13"
            android:text="@string/user_phone"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <EditText
            android:textSize="@dimen/dp_14"
            android:hint="请输入手机号"
            android:digits="1234567890"
            android:inputType="phone|number"
            android:id="@+id/user_phone"
            android:layout_marginRight="@dimen/dp_13"
            android:gravity="right"
            android:background="@null"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <View
        android:layout_marginTop="@dimen/dp_4"
        android:layout_marginLeft="@dimen/dp_13"
        android:background="@color/_E0E0E0"
        android:layout_width="match_parent"
        android:layout_height="1dp">

    </View>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_44">
        <TextView
            android:textColor="@color/_333333"
            android:layout_marginLeft="@dimen/dp_13"
            android:text="@string/user_position"
            android:layout_gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <EditText
            android:textSize="@dimen/dp_14"
            android:hint="请输入职务"
            android:id="@+id/user_position"
            android:layout_marginRight="@dimen/dp_13"
            android:gravity="right"
            android:background="@null"
            android:layout_gravity="center"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <View
        android:layout_marginTop="@dimen/dp_4"
        android:layout_marginLeft="@dimen/dp_13"
        android:background="@color/_E0E0E0"
        android:layout_width="match_parent"
        android:layout_height="1dp">

    </View>
    <TextView
        android:textColor="@color/_000000"
        android:layout_marginTop="@dimen/dp_11"
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/upload_documents" />
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <FrameLayout
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_marginTop="@dimen/dp_30"
            android:orientation="horizontal"
            android:layout_width="@dimen/dp_168"
            android:layout_height="@dimen/dp_88">
            <LinearLayout
                android:id="@+id/ll_legal_positive"
                android:orientation="vertical"
                android:layout_gravity="center"
                android:layout_width="@dimen/dp_118"
                android:layout_height="match_parent"
                android:background="@drawable/shape_dotted_line">
                <ImageView
                    android:layout_marginTop="@dimen/dp_16"
                    android:id="@+id/iv_add"
                    android:layout_gravity="center"
                    android:src="@mipmap/registeradd"
                    android:layout_width="@dimen/dp_26"
                    android:layout_height="@dimen/dp_26" />
                <TextView
                    android:textSize="@dimen/sp_12"
                    android:text="身份证正面上传"
                    android:layout_marginTop="@dimen/dp_8"
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>

            <ImageView
                android:scaleType="centerCrop"
                android:id="@+id/iv_legal_positive"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center" />
        </FrameLayout>
        <FrameLayout
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_marginTop="@dimen/dp_30"
            android:orientation="horizontal"
            android:layout_width="@dimen/dp_168"
            android:layout_height="@dimen/dp_88">
            <LinearLayout
                android:id="@+id/ll_legal_reverse"
                android:orientation="vertical"
                android:layout_gravity="center"
                android:layout_width="@dimen/dp_118"
                android:layout_height="match_parent"
                android:background="@drawable/shape_dotted_line">
                <ImageView
                    android:layout_marginTop="@dimen/dp_16"
                    android:layout_gravity="center"
                    android:src="@mipmap/registeradd"
                    android:layout_width="@dimen/dp_26"
                    android:layout_height="@dimen/dp_26" />
                <TextView
                    android:textSize="@dimen/sp_12"
                    android:text="身份证反面上传"
                    android:layout_marginTop="@dimen/dp_8"
                    android:layout_gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <ImageView
                android:scaleType="centerCrop"
                android:id="@+id/iv_legal_reverse"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                />
        </FrameLayout>
    </LinearLayout>
    <Button
        android:id="@+id/register_register_bt"
        android:layout_width="@dimen/dp_302"
        android:layout_height="@dimen/dp_42"
        android:layout_gravity="center"
        android:layout_marginBottom="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_132"
        android:background="@drawable/shape_login"
        android:text="@string/submit_audit"
        android:textColor="@color/FFFFFF"
        android:textSize="@dimen/sp_14" />
</LinearLayout>