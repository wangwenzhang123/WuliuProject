<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_44"

            android:orientation="horizontal">

            <EditText
                android:id="@+id/et_contact"
                android:layout_width="@dimen/dp_290"
                android:layout_height="@dimen/dp_30"
                android:layout_gravity="right|center"
                android:layout_marginRight="@dimen/dp_16"
                android:background="@null"
                android:gravity="right|center"
                android:hint="@string/input_contact"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_marginLeft="@dimen/dp_16"
                android:text="@string/contact"
                android:textColor="@color/_000000"
                android:textSize="@dimen/sp_14" />
        </FrameLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_marginRight="@dimen/dp_16"
            android:background="@color/_333333">

        </View>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_44"

            android:orientation="horizontal">
            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_marginLeft="@dimen/dp_16"
                android:text="@string/contact_phone"
                android:textColor="@color/_000000"
                android:textSize="@dimen/sp_14" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_weight="1"
                android:text="(登录账号)"
                android:textSize="@dimen/sp_14" />

            <EditText
                android:inputType="numberDecimal"
                android:id="@+id/et_contact_phone"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_30"
                android:layout_gravity="center"
                android:layout_marginRight="@dimen/dp_16"
                android:background="@null"
                android:gravity="right|center"
                android:hint="@string/input_contact_phone"
                android:textSize="@dimen/sp_14" />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_marginRight="@dimen/dp_16"
            android:background="@color/_333333">

        </View>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_44"
            android:orientation="horizontal">

            <EditText
                android:id="@+id/et_address"
                android:layout_width="@dimen/dp_290"
                android:layout_height="@dimen/dp_30"
                android:layout_gravity="right|center"
                android:layout_marginRight="@dimen/dp_16"
                android:background="@null"
                android:gravity="right|center"
                android:hint="@string/input_user_address"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_marginLeft="@dimen/dp_16"
                android:text="@string/address"
                android:textColor="@color/_000000"
                android:textSize="@dimen/sp_14" />
        </FrameLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_marginRight="@dimen/dp_16"
            android:background="@color/_333333">

        </View>
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_44"
            android:orientation="horizontal">

            <EditText
                android:inputType="numberDecimal"
                android:id="@+id/et_driver_age"
                android:layout_width="@dimen/dp_290"
                android:layout_height="@dimen/dp_30"
                android:layout_gravity="right|center"
                android:layout_marginRight="@dimen/dp_16"
                android:background="@null"
                android:gravity="right|center"
                android:hint="请输入驾龄"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_marginLeft="@dimen/dp_16"
                android:text="@string/driver_age"
                android:textColor="@color/_000000"
                android:textSize="@dimen/sp_14" />
        </FrameLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_marginRight="@dimen/dp_16"
            android:background="@color/_333333">

        </View>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_44"

            android:orientation="horizontal">

            <EditText
                android:id="@+id/et_registered_capital"
                android:layout_width="@dimen/dp_290"
                android:layout_height="@dimen/dp_30"
                android:layout_gravity="right|center"
                android:layout_marginRight="@dimen/dp_16"
                android:background="@null"
                android:gravity="right|center"
                android:hint="请输入车牌号"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_marginLeft="@dimen/dp_16"
                android:text="@string/car_no"
                android:textColor="@color/_000000"
                android:textSize="@dimen/sp_14" />
        </FrameLayout>

        <View
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_marginRight="@dimen/dp_16"
            android:background="@color/_333333">

        </View>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:textColor="@color/_333333"
                android:id="@+id/issue_vehicle_type_tv"
                android:text="@string/vehicle_type"
                android:layout_marginTop="@dimen/dp_12"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/dp_17"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <RadioGroup

                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_44">

                <RadioButton
                    android:id="@+id/rb_tong"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginRight="@dimen/dp_24"
                    android:button="@null"
                    android:checked="true"
                    android:drawableLeft="@drawable/selector_order_home"
                    android:drawablePadding="@dimen/dp_8"
                    android:text="@string/concrete_car" />
                <RadioButton
                    android:id="@+id/rb_beng"
                    android:layout_marginRight="@dimen/dp_22"
                    android:layout_gravity="center"
                    android:text="@string/pump_truck"
                    android:button="@null"
                    android:drawablePadding="@dimen/dp_8"
                    android:drawableLeft="@drawable/selector_order_home"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </RadioGroup>
        </LinearLayout>
        <View
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_marginRight="@dimen/dp_16"
            android:background="@color/_333333">

        </View>
        <FrameLayout
            android:id="@+id/tong_ll"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_44"

            android:orientation="horizontal">

            <RadioGroup
                android:id="@+id/rg_type"
                android:layout_gravity="right"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_44">
                <RadioButton
                    android:id="@+id/rg_16"
                    android:checked="true"
                    android:layout_marginRight="@dimen/dp_24"
                    android:layout_gravity="center"
                    android:text="@string/fang_16"
                    android:button="@null"
                    android:drawablePadding="@dimen/dp_8"
                    android:drawableLeft="@drawable/selector_order_home"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <RadioButton
                    android:id="@+id/rg_18"
                    android:layout_marginRight="@dimen/dp_22"
                    android:layout_gravity="center"
                    android:text="@string/fang_18"
                    android:button="@null"
                    android:drawablePadding="@dimen/dp_8"
                    android:drawableLeft="@drawable/selector_order_home"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <RadioButton
                    android:id="@+id/rg_20"
                    android:layout_marginRight="@dimen/dp_22"
                    android:layout_gravity="center"
                    android:text="@string/fang_20"
                    android:button="@null"
                    android:drawablePadding="@dimen/dp_8"
                    android:drawableLeft="@drawable/selector_order_home"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </RadioGroup>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_marginLeft="@dimen/dp_16"
                android:text="@string/car_type"
                android:textColor="@color/_000000"
                android:textSize="@dimen/sp_14" />
        </FrameLayout>
        <View

            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_marginRight="@dimen/dp_16"
            android:background="@color/_333333">

        </View>
        <LinearLayout
            android:visibility="gone"
            android:layout_marginTop="@dimen/dp_12"
            android:layout_marginLeft="@dimen/dp_16"
            android:id="@+id/bang_ll"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout

                android:id="@+id/beng_carType_ll"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_weight="1"
                    android:layout_gravity="center"
                    android:textColor="@color/_333333"
                    android:text="泵车（单位 方）"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <RadioGroup
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dp_44">

                    <RadioButton
                        android:id="@+id/rb_beng_qi"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginRight="@dimen/dp_24"
                        android:button="@null"
                        android:checked="true"
                        android:drawableLeft="@drawable/selector_order_home"
                        android:drawablePadding="@dimen/dp_8"
                        android:text="@string/beng_car" />
                    <RadioButton
                        android:id="@+id/rb_beng_gu"
                        android:layout_marginRight="@dimen/dp_22"
                        android:layout_gravity="center"
                        android:text="@string/beng_gu"
                        android:button="@null"
                        android:drawablePadding="@dimen/dp_8"
                        android:drawableLeft="@drawable/selector_order_home"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RadioGroup>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/beng_qi_ll"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_weight="1"
                    android:layout_gravity="center"
                    android:textColor="@color/_333333"
                    android:text="车辆型号"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <RadioGroup
                    android:layout_gravity="center"
                    android:id="@+id/beng_type"
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dp_44">
                    <RadioButton
                        android:id="@+id/beng_1"
                        android:checked="true"
                        android:layout_marginRight="@dimen/dp_24"
                        android:layout_gravity="center"
                        android:text="@string/beng_1"
                        android:button="@null"
                        android:drawablePadding="@dimen/dp_8"
                        android:drawableLeft="@drawable/selector_order_home"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <RadioButton

                        android:id="@+id/beng_2"
                        android:layout_marginRight="@dimen/dp_22"
                        android:layout_gravity="center"
                        android:text="@string/beng_2"
                        android:button="@null"
                        android:drawablePadding="@dimen/dp_8"
                        android:drawableLeft="@drawable/selector_order_home"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <RadioButton

                        android:id="@+id/beng_3"
                        android:layout_marginRight="@dimen/dp_22"
                        android:layout_gravity="center"
                        android:text="@string/beng_3"
                        android:button="@null"
                        android:drawablePadding="@dimen/dp_8"
                        android:drawableLeft="@drawable/selector_order_home"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RadioGroup>
            </LinearLayout>
            <LinearLayout
                android:visibility="gone"
                android:id="@+id/beng_gu_ll"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_weight="1"
                    android:layout_gravity="center"
                    android:textColor="@color/_333333"
                    android:text="车辆型号"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <RadioGroup
                    android:id="@+id/beng_gu_type"
                    android:layout_gravity="center"
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dp_44">
                    <RadioButton
                        android:id="@+id/gu_1"
                        android:checked="true"
                        android:layout_marginRight="@dimen/dp_24"
                        android:layout_gravity="center"
                        android:text="@string/putong"
                        android:button="@null"
                        android:drawablePadding="@dimen/dp_8"
                        android:drawableLeft="@drawable/selector_order_home"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                    <RadioButton
                        android:id="@+id/gu_2"
                        android:layout_marginRight="@dimen/dp_22"
                        android:layout_gravity="center"
                        android:text="@string/gaoya"
                        android:button="@null"
                        android:drawablePadding="@dimen/dp_8"
                        android:drawableLeft="@drawable/selector_order_home"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </RadioGroup>
            </LinearLayout>

        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_marginRight="@dimen/dp_16"
            android:background="@color/_333333">

        </View>
        <FrameLayout
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_44"

            android:orientation="horizontal">

            <EditText
                android:inputType="numberDecimal"
                android:id="@+id/et_enterprise_number"
                android:layout_width="@dimen/dp_290"
                android:layout_height="@dimen/dp_30"
                android:layout_gravity="right|center"
                android:layout_marginRight="@dimen/dp_16"
                android:background="@null"
                android:gravity="right|center"
                android:hint="请输入载重"
                android:textSize="@dimen/sp_14" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_marginLeft="@dimen/dp_16"
                android:text="@string/load"
                android:textColor="@color/_000000"
                android:textSize="@dimen/sp_14" />
        </FrameLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_marginRight="@dimen/dp_16"
            android:background="@color/_333333">

        </View>
        <TextView
            android:textColor="@color/_000000"
            android:layout_marginTop="@dimen/dp_11"
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/upload_documents" />
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <FrameLayout
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_marginTop="@dimen/dp_30"
                android:orientation="horizontal"
                android:layout_width="@dimen/dp_168"
                android:layout_height="@dimen/dp_88">
                <LinearLayout
                    android:id="@+id/ll_legal_positive"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:layout_width="@dimen/dp_118"
                    android:layout_height="match_parent"
                    android:background="@drawable/shape_dotted_line">
                    <ImageView
                        android:layout_marginTop="@dimen/dp_16"
                        android:id="@+id/iv_add"
                        android:layout_gravity="center"
                        android:src="@mipmap/registeradd"
                        android:layout_width="@dimen/dp_26"
                        android:layout_height="@dimen/dp_26" />
                    <TextView
                        android:textSize="@dimen/sp_12"
                        android:text="身份证正面上传"
                        android:layout_marginTop="@dimen/dp_8"
                        android:layout_gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <ImageView
                    android:id="@+id/iv_legal_positive"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:scaleType="centerCrop" />
            </FrameLayout>
            <FrameLayout
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_marginTop="@dimen/dp_30"
                android:orientation="horizontal"
                android:layout_width="@dimen/dp_168"
                android:layout_height="@dimen/dp_88">
                <LinearLayout
                    android:id="@+id/ll_legal_reverse"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:layout_width="@dimen/dp_118"
                    android:layout_height="match_parent"
                    android:background="@drawable/shape_dotted_line">
                    <ImageView
                        android:layout_marginTop="@dimen/dp_16"
                        android:layout_gravity="center"
                        android:src="@mipmap/registeradd"
                        android:layout_width="@dimen/dp_26"
                        android:layout_height="@dimen/dp_26" />
                    <TextView
                        android:textSize="@dimen/sp_12"
                        android:text="身份证反面上传"
                        android:layout_marginTop="@dimen/dp_8"
                        android:layout_gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <ImageView
                    android:scaleType="centerCrop"
                    android:id="@+id/iv_legal_reverse"
                    android:layout_gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    />


            </FrameLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <FrameLayout
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_marginTop="@dimen/dp_30"
                android:orientation="horizontal"
                android:layout_width="@dimen/dp_168"
                android:layout_height="@dimen/dp_88">
                <LinearLayout
                    android:id="@+id/ll_legal_positive1"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:layout_width="@dimen/dp_118"
                    android:layout_height="match_parent"
                    android:background="@drawable/shape_dotted_line">
                    <ImageView
                        android:layout_marginTop="@dimen/dp_16"
                        android:id="@+id/iv_add1"
                        android:layout_gravity="center"
                        android:src="@mipmap/registeradd"
                        android:layout_width="@dimen/dp_26"
                        android:layout_height="@dimen/dp_26" />
                    <TextView
                        android:textSize="@dimen/sp_12"
                        android:text="驾照上传"
                        android:layout_marginTop="@dimen/dp_8"
                        android:layout_gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <ImageView
                    android:id="@+id/iv_legal_positive1"
                    android:layout_width="@dimen/dp_118"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:background="@drawable/shape_dotted_line"
                    android:scaleType="centerCrop" />
            </FrameLayout>
            <FrameLayout
                android:visibility="gone"
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_marginTop="@dimen/dp_30"
                android:orientation="horizontal"
                android:layout_width="@dimen/dp_168"
                android:layout_height="@dimen/dp_88">
                <LinearLayout
                    android:id="@+id/ll_legal_reverse1"
                    android:orientation="vertical"
                    android:layout_gravity="center"
                    android:layout_width="@dimen/dp_118"
                    android:layout_height="match_parent"
                    android:background="@drawable/shape_dotted_line">
                    <ImageView
                        android:layout_marginTop="@dimen/dp_16"
                        android:layout_gravity="center"
                        android:src="@mipmap/registeradd"
                        android:layout_width="@dimen/dp_26"
                        android:layout_height="@dimen/dp_26" />
                    <TextView
                        android:textSize="@dimen/sp_12"
                        android:text="行驶证上传"
                        android:layout_marginTop="@dimen/dp_8"
                        android:layout_gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content" />
                </LinearLayout>
                <ImageView
                    android:scaleType="fitXY"
                    android:id="@+id/iv_legal_reverse1"
                    android:layout_gravity="center"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/shape_dotted_line"
                    />
            </FrameLayout>
        </LinearLayout>
        <Button
            android:id="@+id/register_register_bt"
            android:layout_width="@dimen/dp_302"
            android:layout_height="@dimen/dp_42"
            android:layout_below="@+id/view4"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/dp_20"
            android:layout_marginTop="@dimen/dp_42"
            android:background="@drawable/shape_login"
            android:text="@string/submit_audit"
            android:textColor="@color/FFFFFF"
            android:textSize="@dimen/sp_14" />
    </LinearLayout>
</android.support.v4.widget.NestedScrollView>